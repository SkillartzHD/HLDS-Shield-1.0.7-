stock BufferName(info[],size,name[],size_name = 31) // nu pune mai mare decat 31
{
	new heh = 0x00;
	if ((heh = containi(info, "\name\") ) != -1){
		heh += 0x06;
		new len = 0x00;
		while(info[heh] != '\' && info[heh] != '"' && len < size_name && heh < size)
			name[len++] = info[heh++];
		
		name[len] = EOS;
	}
	else{
		name[0x00] = EOS;
	}
}
